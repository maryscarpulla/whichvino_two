<div class="page-header">
  <h3>My Wine #<%= @my_wine.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>User</dt>
      <dd><%= @my_wine.user_id %></dd>

      <dt>Wine</dt>
      <dd><%= @my_wine.wine_id %></dd>

      <dt>Bucket list</dt>
      <dd><%= @my_wine.bucket_list %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/my_wines" class="btn btn-primary">
        Back
      </a>
      <a href="/my_wines/<%= @my_wine.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_my_wine/<%= @my_wine.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
